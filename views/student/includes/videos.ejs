<div class="content-one">
    <div class="flex f-space-between p-4">
        <h2>Video</h2>
    </div>
    <div class="cards grid sm:grid-cols-2 lg:grid-cols-3 gap-4 my-3">
        <% lesson.videos.forEach(v =>{ %>
        <div class="card item">
            <input type="hidden" name="examId" value="<%= lesson._id%>" id="">
            <img src="/images/videoIcon.jpg" style="max-height: 70%; margin: auto;" class="card-img" alt="...">
            <h2 class="card-title text-m" style="text-align: center;">
                <%= v.title %>
            </h2>
                <a class="btn btn-main watch-vid" style="max-height: 70%; max-width: 50%; margin: auto; display: block;" data-vn="<%= v.title %>" data-vp="<%= v.path %>">Watch</a>
        </div>
        <% }) %>
    </div>


</div>